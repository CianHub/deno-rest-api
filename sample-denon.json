{
  "$schema": "https://deno.land/x/denon/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run server.ts",
      "allow": ["env", "net"],
      "env": {
        "PORT": "3000",
        "dbUser": "username",
        "dbName": "dbname",
        "dbPassword": "password",
        "dbHostname": "hostname",
        "dbPort": "port"
      },
      "desc": "run my app.ts file"
    }
  }
}
